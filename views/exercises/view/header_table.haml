%table.header_table.h3
  %tbody
    %tr
      %td.exercise_name
        = "#{@exercise.sort_order}." if @exercise.sort_order
        = @exercise.name
      - if @exercise.will_be_interactive?
        %td.exercise_note= "<i class=\"fa fa-star\" title=\"#{I18n.t('views.fake.will_be_interactive')}\"></i>"
